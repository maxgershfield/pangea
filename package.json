{
	"name": "pangea-markets-backend",
	"version": "1.0.0",
	"type": "module",
	"description": "Pangea Markets RWA Trading Platform Backend",
	"author": "Pangea Markets",
	"private": true,
	"scripts": {
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main.js",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:cov": "vitest run --coverage",
		"typeorm": "tsx ./node_modules/typeorm/cli.js",
		"migration:generate": "npm run typeorm migration:generate -- -d src/config/data-source.ts",
		"migration:run": "npm run typeorm migration:run -- -d src/config/data-source.ts",
		"migration:revert": "npm run typeorm migration:revert -- -d src/config/data-source.ts",
		"knip": "knip",
		"knip:fix": "knip --fix"
	},
	"dependencies": {
		"@nestjs/common": "11.1.11",
		"@nestjs/config": "4.0.2",
		"@nestjs/core": "11.1.11",
		"@nestjs/jwt": "11.0.2",
		"@nestjs/mapped-types": "^2.0.4",
		"@nestjs/platform-express": "11.1.11",
		"@nestjs/platform-socket.io": "11.1.11",
		"@nestjs/schedule": "6.1.0",
		"@nestjs/typeorm": "11.0.0",
		"@nestjs/websockets": "11.1.11",
		"axios": "^1.6.5",
		"bs58": "^5.0.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"ethers": "^5.7.2",
		"form-data": "^4.0.0",
		"ioredis": "^5.3.2",
		"jose": "^6.1.3",
		"pg": "^8.11.3",
		"redis": "^4.6.12",
		"reflect-metadata": "0.2.2",
		"rxjs": "^7.8.1",
		"socket.io": "^4.6.1",
		"tweetnacl": "^1.0.3",
		"typeorm": "^0.3.17"
	},
	"devDependencies": {
		"@nestjs/cli": "11.0.14",
		"@nestjs/schematics": "11.0.9",
		"@nestjs/testing": "11.1.11",
		"@swc/core": "^1.15.8",
		"@types/express": "^5.0.6",
		"@types/node": "25.0.3",
		"@typescript-eslint/eslint-plugin": "8.51.0",
		"@typescript-eslint/parser": "8.51.0",
		"@vitest/coverage-v8": "4.0.16",
		"dotenv": "^17.2.3",
		"eslint": "9.39.2",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "5.5.4",
		"knip": "^5.78.0",
		"prettier": "^3.1.1",
		"source-map-support": "^0.5.21",
		"tsx": "^4.7.0",
		"typescript": "^5.3.3",
		"unplugin-swc": "^1.5.9",
		"vitest": "4.0.16"
	},
	"optionalDependencies": {
		"@types/better-sqlite3": "^7.6.13",
		"better-sqlite3": "^12.5.0"
	}
}
